<Application x:Class="FeedbackExtractor.Client.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:FeedbackExtractor.Client" 
             xmlns:conv="clr-namespace:FeedbackExtractor.Client.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BooleanToVisibilityConverter x:Key="VisibilityConverter" />
        <conv:BoolToOppositeBoolConverter x:Key="OppositeBoolConverter" />
        <conv:VisibilityToOppositeBoolConverter x:Key="VisibilityToOppositeBoolConverter" />
        <conv:NotNullToVisibilityConverter x:Key="NotNullToVisibilityConverter" />
    </Application.Resources>
</Application>
